<mvc:View height="100%" controllerName="ui5.typescript.helloworld.controller.inspection.Detail"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:tnt="sap.tnt">
  <Page showHeader="false">
    <landmarkInfo>
      <PageAccessibleLandmarkInfo rootRole="Region" rootLabel="{i18n>Statistics_rootLabel}" contentRole="Main" contentLabel="{i18n>Statistics_contentLabel}"/>
    </landmarkInfo>
    <content>
      <Panel id="statisticsContainer" accessibleRole="Region" class="sapUiNoContentPadding">
        <headerToolbar>
          <Toolbar>
            <Title text="{path:'/data/sip_id'}" level="H3" textAlign="Left"/>
            <ToolbarSpacer/>
            <Button id="refresh" icon="sap-icon://refresh" press=".onRefresh" tooltip="{i18n>refreshButtonTooltip}"/>
          </Toolbar>
        </headerToolbar>
      </Panel>
      <Text text="{path:'/data/sip_id'}"></Text>
    </content>
  </Page>
</mvc:View>